<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}Home Page |{% endblock %} </title>

    <!-- Bootstrap core CSS -->
    {% block stylesheets %}
        {% stylesheets filter='cssrewrite' 'Bundles/app/gentelella/css/bootstrap.min.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}
        {% stylesheets filter='cssrewrite' 'Bundles/app/gentelella/fonts/css/font-awesome.min.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}

        <link href="http://fontawesome.io/assets/font-awesome/css/font-awesome.css" rel="stylesheet">
        {% stylesheets filter='cssrewrite' 'Bundles/app/gentelella/css/animate.min.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}
        {% stylesheets filter='cssrewrite' 'Bundles/app/gentelella/css/custom.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}
        {% stylesheets filter='cssrewrite' 'Bundles/app/gentelella/css/icheck/flat/green.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}
        {% stylesheets filter='cssrewrite' 'Bundles/app/gentelella/css/maps/jquery-jvectormap-2.0.3.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}
        {% stylesheets filter='cssrewrite' 'Bundles/app/gentelella/css/floatexamples.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}

    {% endblock %}



</head>


<body class="nav-md">

<div class="container body">


    <div class="main_container">

        <div class="col-md-3 left_col">
            <div class="left_col scroll-view">

                <div class="navbar nav_title" style="border: 0;">
                    <a href="{{ path('testgentela') }}" class="site_title"><i class="fa fa-group"></i> <span>STEG!</span></a>
                </div>
                <div class="clearfix"></div>

                <!-- menu prile quick info -->
                {% block info %}
                <div class="profile">
                    <div class="profile_pic">
                        {% image 'Bundles/app/gentelella/images/img.jpg' %}
                        <img src="{{ asset_url }}" alt="..." class="img-circle profile_img" />
                        {% endimage %}

                    </div>
                    <div class="profile_info">
                        <span>Welcome,</span>
                        <h2>{% if is_granted('IS_AUTHENTICATED_FULLY') %}
                                {{ app.user.username }}
                            {% endif %}</h2>
                    </div>
                </div>
                {% endblock %}
                <!-- /menu prile quick info -->

                <br />

                <!-- sidebar menu -->
                {% block sidebar %}
                <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">

                    <div class="menu_section">
                        <h3>General</h3>
                        <ul class="nav side-menu">
                            {% if access[0]==true %}
                            <li><a><i class="fa fa-group"></i> Gestion Administration <span class="fa fa-chevron-down"></span></a>
                                <ul class="nav child_menu" style="display: none">
                                    <li><a href="{{ path('afficheralluser') }}">Afficher Utilisateurs</a>
                                    </li>
                                    <li><a href="{{ path('afficherole') }}">Gestion Roles</a>
                                    </li>
                                    <li><a href="{{ path('afficheprofile') }}">Gestion Profil</a>
                                    </li>

                                    </li>
                                </ul>
                            </li>
                            {% endif %}
                            {% if access[1]==true %}
                            <li><a><i class="fa fa-cubes"></i> Gestion Stock <span class="fa fa-chevron-down"></span></a>
                                <ul class="nav child_menu" style="display: none">
                                    <li><a href="{{ path('affichearticles') }}">Gestion Article</a>
                                    </li>
                                    <li><a href="{{ path('afficherbon') }}">Gestion bon d'article</a>
                                    </li>
                                    <li><a href="{{ path('affichestation') }}">Gestion Station</a>
                                    </li>
                                    </li>
                                </ul>
                            </li>
                            {% endif %}
                            {% if access[2]==true %}
                            <li><a><i class="fa fa-edit"></i> Gestion Interventions <span class="fa fa-chevron-down"></span></a>
                                <ul class="nav child_menu" style="display: none">
                                    <li><a href="{{ path('afficher_all_intervention')}}">Afficher Interventions</a>
                                    </li>
                                    <li><a href="{{ path('afficherhotel') }}">Afficher Hotels</a>
                                    </li>
                                    <li><a href="{{ path('afficherbonhotel') }}">Afficher Bons Hotels</a>
                                    </li>
                                    <li><a href="{{ path('affichervol') }}">Afficher Vols</a>
                                    </li>
                                    <li><a href="{{ path('afficherbonvol') }}">Afficher Bons Vols</a>
                                    </li>
                                    <li><a href="{{ path('afficherdiffusion') }}">Afficher Diffusion</a>
                                    </li>

                                </ul>
                            </li>
                            {% endif %}
                            {% if access[3]==true %}
                            <li><a><i class="fa fa-crosshairs " ></i> Gestion Mission <span class="fa fa-chevron-down"></span></a>
                                <ul class="nav child_menu" style="display: none">
                                    <li><a href="{{ path('afficherallmission') }}">Afficher Missions</a>
                                    </li>
                                    <li><a href="{{ path('affichervehicule') }}">Afficher VÃ©hicules</a>
                                    </li>

                                </ul>
                            </li>
                            {% endif %}
                            {% if access[4]==true %}
                            <li><a><i class="fa fa-history"></i> Gestion Historique <span class="fa fa-chevron-down"></span></a>
                                <ul class="nav child_menu" style="display: none">
                                    <li><a href="{{ path('historiquegenerale') }}">Consulter Historique Globale</a>
                                    </li>
                                    <li><a href="{{ path('historiquestock') }}">Consulter Historique Stock</a>
                                    </li>
                                    <li><a href="{{ path('historiqueintervention') }}">Consulter Historique Intervention</a>
                                    </li>
                                    <li><a href="{{ path('historiquemission') }}">Consulter Historique Mission</a>
                                    </li>
                                    <li><a href="{{ path('historiqueadministration') }}">Consulter Historique Administration</a>
                                    </li>
                                    </li>
                                </ul>
                            </li>
                            {% endif %}
                            {% if access[5]==true %}
                            <li><a><i class="fa fa-dashboard "></i> Gestion Reporting <span class="fa fa-chevron-down"></span></a>
                                <ul class="nav child_menu" style="display: none">
                                    <li><a href="{{ path('stat_intervention_intervenant') }}">Consulter Statistique Generale</a>
                                    </li>
                                </ul>
                            </li>
                            {% endif %}
                        </ul>
                    </div>


                </div>
                {% endblock %}
                <!-- /sidebar menu -->

                <!-- /menu footer buttons -->
                {% block footerbuttons %}
                <div class="sidebar-footer hidden-small">
                    <a data-toggle="tooltip" data-placement="top" title="Settings">
                        <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                    </a>
                    <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                        <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                    </a>
                    <a data-toggle="tooltip" data-placement="top" title="Lock">
                        <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                    </a>
                    <a data-toggle="tooltip" data-placement="top" title="Logout" href="{{ path('re-logout') }}">
                        <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                    </a>
                </div>
                {% endblock %}
                <!-- /menu footer buttons -->
            </div>
        </div>

        <!-- top navigation -->
        {% block topnavigation %}
        <div class="top_nav">

            <div class="nav_menu">
                <nav class="" role="navigation">
                    <div class="nav toggle">
                        <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                    </div>

                    <ul class="nav navbar-nav navbar-right">
                        <li class="">
                            <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                {% image 'Bundles/app/gentelella/images/img.jpg' %}
                                <img src="{{ asset_url }}" alt=""  />
                                {% endimage %}{% if is_granted('IS_AUTHENTICATED_FULLY') %}
                                    {{ app.user.username }}
                                {% endif %}
                                <span class=" fa fa-angle-down"></span>
                            </a>
                            <ul class="dropdown-menu dropdown-usermenu pull-right">

                                <li>
                                    <a href="javascript:;">Help</a>
                                </li>
                                <li><a href="{{ path('re-logout') }}"><i class="fa fa-sign-out pull-right"></i> Log Out</a>
                                </li>
                            </ul>
                        </li>



                    </ul>
                </nav>
            </div>

        </div>
        {% endblock %}
        <!-- /top navigation -->

        {% block body %}

        {% endblock %}
        {% block footer %}
            <footer>
                <div class="pull-right">
                    STEG - Direction de la Production et du Transport Gaz
                </div>
                <div class="clearfix"></div>
            </footer>
        {% endblock %}

    </div>
</div>



{% block javascripts %}

        {% javascripts 'Bundles/app/gentelella/js/jquery.min.js' output='js/compiled/jquery.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts 'Bundles/app/gentelella/js/nprogress.js' output='js/nprogress.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

    {% javascripts 'Bundles/app/gentelella/js/bootstrap.min.js' output='js/bootstrap.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

        <!-- gauge js -->
        {% javascripts '@AppBundle/Resources/public/gentelella/js/gauge/gauge.min.js' output='js/gauge/gauge.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/gauge/gauge_demo.js' output='js/gauge/gauge_demo.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        <!-- bootstrap progress js -->
        {% javascripts '@AppBundle/Resources/public/gentelella/js/progressbar/bootstrap-progressbar.min.js' output='js/progressbar/bootstrap-progressbar.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <!-- icheck -->
        {% javascripts '@AppBundle/Resources/public/gentelella/js/icheck/icheck.min.js' output='js/icheck/icheck.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <!-- daterangepicker -->
        {% javascripts '@AppBundle/Resources/public/gentelella/js/moment/moment.min.js' output='js/moment/moment.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/datepicker/daterangepicker.js' output='js/datepicker/daterangepicker.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}


        <!-- chart js -->
        {% javascripts '@AppBundle/Resources/public/gentelella/js/chartjs/chart.min.js' output='js/chartjs/chart.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/custom.js' output='js/custom.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}



        <!-- flot js -->
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/jquery.flot.js' output='js/flot/jquery.flot.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/jquery.flot.pie.js' output='js/flot/jquery.flot.pie.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/jquery.flot.orderBars.js' output='js/flot/jquery.flot.orderBars.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/jquery.flot.time.min.js' output='js/flot/jquery.flot.time.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/date.js' output='js/flot/date.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/jquery.flot.spline.js' output='js/flot/jquery.flot.spline.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/jquery.flot.stack.js' output='js/flot/jquery.flot.stack.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/curvedLines.js' output='js/flot/curvedLines.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/gentelella/js/flot/jquery.flot.resize.js' output='js/flot/jquery.flot.resize.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

    <!-- worldmap -->
    {% javascripts '@AppBundle/Resources/public/gentelella/js/maps/jquery-jvectormap-2.0.3.min.js' output='js/maps/jquery-jvectormap-2.0.3.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% javascripts '@AppBundle/Resources/public/gentelella/js/maps/gdp-data.js' output='js/maps/gdp-data.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% javascripts '@AppBundle/Resources/public/gentelella/js/maps/jquery-jvectormap-world-mill-en.js' output='js/maps/jquery-jvectormap-world-mill-en.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% javascripts '@AppBundle/Resources/public/gentelella/js/maps/jquery-jvectormap-us-aea-en.js' output='js/maps/jquery-jvectormap-us-aea-en.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}


    <!-- pace -->
    {% javascripts '@AppBundle/Resources/public/gentelella/js/pace/pace.min.js' output='js/pace/pace.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <script>
        $(function() {
            $('#world-map-gdp').vectorMap({
                map: 'world_mill_en',
                backgroundColor: 'transparent',
                zoomOnScroll: false,
                series: {
                    regions: [{
                        values: gdpData,
                        scale: ['#E6F2F0', '#149B7E'],
                        normalizeFunction: 'polynomial'
                    }]
                },
                onRegionTipShow: function(e, el, code) {
                    el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
                }
            });
        });
    </script>
    <!-- skycons -->
    {% javascripts '@AppBundle/Resources/public/gentelella/js/skycons/skycons.min.js' output='js/skycons/skycons.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <script>
        var icons = new Skycons({
                "color": "#73879C"
            }),
            list = [
                "clear-day", "clear-night", "partly-cloudy-day",
                "partly-cloudy-night", "cloudy", "rain", "sleet", "snow", "wind",
                "fog"
            ],
            i;

        for (i = list.length; i--;)
            icons.set(list[i], list[i]);

        icons.play();
    </script>

    <script>
        NProgress.done();
    </script>

{% endblock %}
</body>
</html>
