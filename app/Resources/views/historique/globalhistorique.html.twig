        {% endstylesheets %}

        {% stylesheets 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}
        {% stylesheets 'https://cdn.datatables.net/1.10.13/css/dataTables.bootstrap.min.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}

    {% endblock %}


{% block body %}
    <div class="right_col" role="main">
        <div class="">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>Afficher Historiques Globale <small>Gestion Historique</small></h2>
                            <ul class="nav navbar-right panel_toolbox">
                                <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                </li>
                                <li><a class="close-link"><i class="fa fa-close"></i></a>
                                </li>
                            </ul>
                            <div class="clearfix"></div>
                        </div>

                        <br><br><br>

                            <div class="row">
                                <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                                    <thead>
                                    <tr>
                                        <th><center>Date</center></th>
                                        <th><center>Utilisateur</center></th>
                                        <th><center>Type</center></th>
                                        <th><center>Champ</center></th>
                                        <th><center>Mise à jour</center></th>
                                        <th><center>Module</center></th>


                                    </tr>
                                    </thead>
                                    <tbody>
                                    {% for his in historique %}

                                        <tr>

                                            <td><center> {{ his.getDatehistorique() }} </center> </td>
                                            <td><center> {{ his.getUtilisateur().getPrenom() }} {{ his.getUtilisateur().getNom() }} </center> </td>
                                            <td><center> {{ his.getType() }} </center></td>
                                            <td><center> {{ his.getChamp() }} </center></td>
                                            <td><center> changement de {{ his.getEncienvaleur() }} à {{ his.getNouveauvaleur() }} </center></td>
                                            <td><center> {{ his.getModule() }} </center></td>
                                        </tr>
                                    {% endfor %}
                                    </tbody>

                                </table>


                            </div></div></div></div>
            </div>
        </div>
    </div>









{% endblock %}

