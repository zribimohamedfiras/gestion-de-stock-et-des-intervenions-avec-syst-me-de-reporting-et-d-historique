<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Steg!{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets 'Bundles/app/css/styles.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}">
            {% endstylesheets %}

            {% stylesheets 'Bundles/app/gentelella/css/bootstrap.min.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}">
            {% endstylesheets %}
            <link href="http://fontawesome.io/assets/font-awesome/css/font-awesome.css" rel="stylesheet">
            {% stylesheets 'Bundles/app/gentelella/css/animate.min.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}">
            {% endstylesheets %}
            {% stylesheets 'Bundles/app/gentelella/css/custom.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}">
            {% endstylesheets %}
            {% stylesheets 'Bundles/app/gentelella/css/icheck/flat/green.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}">
            {% endstylesheets %}




        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body style="background:#F7F7F7;">
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="//code.jquery.com/jquery-2.1.4.min.js"></script>
            {% javascripts 'Bundles/app/gentelella/js/jquery.min.js' output='js/compiled/jquery.min.js' %}
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
            <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
            {% javascripts '@AppBundle/Resources/public/js/main.js' output='js/compiled/main.js' %}
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
